/* -- the background color for the whole page and the borders for the topic page, header, footer */
/* -- IMPORTANT -- color is the color of the text on div.document;
	 must be $amperity-brand-slate or $amperity-brand-slate */
body {
  font-family: "Montserrat", "Open+Sans", "Helvetica Neue", "Helvetica", Arial, sans-serif;
  font-weight: 400;
  font-size: 14px;
  letter-spacing: -0.00em;
  line-height: 1.5;
  margin-left: 0px;
  background-color: #ffffff;
  color: #0c0c0c;
  width: 100%;
  -webkit-font-smoothing: antialiased;
  overflow-x: hidden;
  min-width: 270px; }

/* -- the actual topic background, must be white all the time no exceptions */
div.document {
  background-color: #ffffff;
  text-align: left;
  background-image: url(contents.png);
  background-repeat: repeat-x;
  max-width: 1295px;
  margin: 0 auto;
  width: 100%; }

/* -- the vertical line between the topics and the TOC */
div.bodywrapper {
  margin: 0; }
  @media (min-width: 1100px) {
    div.bodywrapper {
      margin-right: 270px; } }
  div.bodywrapper img {
    max-width: 100%; }

div.documentwrapper {
  background: #ffffff; }

div.body-wrap {
  width: 100%;
  overflow: hidden;
  background: white; }

div.body {
  margin: 0;
  padding: 6px 20px 20px 28px; }

div.related {
  font-size: 1em; }

/* -- the borders above and below the header and footer */
div.related ul {
  background-color: #ffffff;
  height: 1.8em;
  border-top: 4px solid #f2f4f4;
  border-bottom: 4px solid #f2f4f4; }

div.related ul li {
  margin: 0;
  padding: 0;
  height: 2em;
  float: left; }

div.related ul li.right {
  float: right;
  margin-right: 5px; }

/* -- the Sphinx-specific navigation text located just below the header and just above the footer */
div.related ul li a {
  margin: 0;
  padding: 0 8px 0 5px;
  line-height: 1.75em;
  color: #5a5f66; }

div.related ul li a:hover {
  color: #8fdd7a; }

div.sphinxsidebarwrapper {
  padding: 0;
  margin-top: 10px; }

div.sphinxsidebarwrapper > ul > li {
  list-style-type: none; }

div.sphinxsidebar {
  display: none;
  position: relative;
  z-index: 1;
  margin: 0;
  padding: 0.5em 15px 15px 0;
  width: 275px;
  float: right;
  font-size: 1em;
  text-align: left; }
  @media (min-width: 1100px) {
    div.sphinxsidebar {
      display: block;
      padding-left: 10px; } }

/* -- The colors of the TOC sidebar; h3 is headers, a is the text links and both should be same color */
div.sphinxsidebar h2 {
  margin: 1em 0 0.5em 0;
  font-size: 1em;
  font-weight: 600;
  padding: 0.0em 0 0.0em 0.0em;
  color: #0c0c0c;
  background-color: #ffffff;
  border-bottom: 0px; }

div.sphinxsidebar h3, div.sphinxsidebar h4 {
  margin: 1em 0 0.5em 0;
  font-size: 1em;
  font-weight: 400;
  padding: 0.2em 0 0.1em 0.6em;
  color: #0c0c0c;
  background-color: #ffffff;
  border-bottom: 0px; }

div.sphinxsidebar a {
  padding: 0em 0 0em 0em;
  text-decoration: none; }

div.sphinxsidebar h3 a {
  color: #ffffff; }

div.sphinxsidebar ul {
  padding-left: 1.5em;
  padding: 0;
  line-height: 160%;
  list-style-type: square; }

div.sphinxsidebar ul ul {
  margin-left: 22px; }

div.sphinxsidebar li {
  position: relative;
  left: .2em; }

div.sphinxsidebar li li {
  position: relative;
  left: .2em; }

